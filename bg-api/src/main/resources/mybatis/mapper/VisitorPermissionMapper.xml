<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chinamobile.iot.xiaoyan.bgapi.mapper.VisitorPermissionMapper">
    <resultMap id="VisitorPermissionResultMap" type="VisitorPermission">
        <id column="id" property="id"/>
        <result column="description" property="description"/>
        <result column="function" property="function"/>
    </resultMap>

    <select id="listAll" resultMap="VisitorPermissionResultMap">
        select * from visitor_permissions
    </select>

</mapper>
