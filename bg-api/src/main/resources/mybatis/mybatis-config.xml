<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <setting name="aggressiveLazyLoading" value="false"/>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer"/>
        <typeAlias alias="Long" type="java.lang.Long"/>
        <typeAlias alias="HashMap" type="java.util.HashMap"/>
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap"/>
        <typeAlias alias="ArrayList" type="java.util.ArrayList"/>
        <typeAlias alias="LinkedList" type="java.util.LinkedList"/>
        <typeAlias alias="Developer" type="com.chinamobile.iot.xiaoyan.bgapi.entity.Developer"/>
        <typeAlias alias="Skill" type="com.chinamobile.iot.xiaoyan.bgapi.entity.Skill"/>
        <typeAlias alias="SkillExample" type="com.chinamobile.iot.xiaoyan.bgapi.entity.SkillExample"/>
        <typeAlias alias="FeaturedContent" type="com.chinamobile.iot.xiaoyan.bgapi.entity.FeaturedContent"/>
        <typeAlias alias="ResourceAddress" type="com.chinamobile.iot.xiaoyan.bgapi.entity.ResourceAddress"/>
        <typeAlias alias="MiguVoice" type="com.chinamobile.iot.xiaoyan.bgapi.entity.MiguVoice"/>
        <typeAlias alias="User" type="com.chinamobile.iot.xiaoyan.bgapi.entity.User"/>
        <typeAlias alias="Role" type="com.chinamobile.iot.xiaoyan.bgapi.entity.Role"/>
        <typeAlias alias="Permission" type="com.chinamobile.iot.xiaoyan.bgapi.entity.Permission"/>
        <typeAlias alias="AccessToken" type="com.chinamobile.iot.xiaoyan.bgapi.entity.AccessToken"/>
        <typeAlias alias="VisitorPermission" type="com.chinamobile.iot.xiaoyan.bgapi.entity.VisitorPermission"/>
        <typeAlias alias="MiguCategory" type="com.chinamobile.iot.xiaoyan.bgapi.entity.MiguCategory"/>

    </typeAliases>

    <plugins>
        <plugin interceptor="com.chinamobile.iot.xiaoyan.bgapi.plugin.PageInterceptor">
            <property name="dialect" value="com.chinamobile.iot.xiaoyan.bgapi.plugin.MySqlDialect"/>
        </plugin>
    </plugins>

</configuration>